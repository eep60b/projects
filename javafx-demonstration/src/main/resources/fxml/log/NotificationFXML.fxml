<AnchorPane id="AnchorPane" prefHeight="150.0" prefWidth="400.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.etlsolutions.javafx.presentation.dialog.log.NotificationFXMLController">
    <stylesheets>
        <URL value="@/styles/log/notificationfxml.css" />
    </stylesheets>
   <children>
      <VBox prefHeight="194.0" prefWidth="400.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
