            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <HBox fx:id="notificationHbox" spacing="10.0">
               <children>
                  <Label text="Notification:" />
                  <ComboBox fx:id="timeUnitComboBox" prefHeight="25.0" prefWidth="305.0" />
               </children>
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER_RIGHT" layoutX="20.0" layoutY="50.0" spacing="10.0">
               <children>
                  <Button fx:id="okButton" mnemonicParsing="false" text="OK" />
                  <Button fx:id="cancelButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="Cancel" />
               </children>
               <VBox.margin>
                  <Insets bottom="10.0" top="50.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
