<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.etlsolutions.javafx.presentation.area.subarea.location.GroundLocationFXMLController">
    <stylesheets>
        <URL value="@/styles/area/borderlocationfxml.css" />
    </stylesheets>
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox spacing="10.0">
               <children>
                  <Label fx:id="referencePointLabel" prefHeight="17.0" prefWidth="174.0" text="Reference point in the area part">
                     <HBox.margin>
                        <Insets left="10.0" top="3.0" />
                     </HBox.margin>
                  </Label>
                  <ComboBox fx:id="referencePointComboBox" prefHeight="25.0" prefWidth="400.0" />
               </children>
               <padding>
                  <Insets top="10.0" />
               </padding>
            </HBox>
            <HBox layoutX="10.0" layoutY="10.0" spacing="10.0">
               <children>
                  <Label fx:id="directionLabel" prefHeight="17.0" prefWidth="174.0" text="Direction">
                     <HBox.margin>
                        <Insets left="10.0" top="3.0" />
                     </HBox.margin>
                  </Label>
                  <ComboBox fx:id="directionComboBox" prefHeight="25.0" prefWidth="400.0" />
               </children>
            </HBox>
            <HBox layoutX="10.0" layoutY="55.0" spacing="10.0">
               <children>
                  <Label fx:id="distanceLabel" prefHeight="17.0" prefWidth="174.0" text="Distance">
                     <HBox.margin>
                        <Insets left="10.0" top="3.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="distanceTextField" HBox.hgrow="ALWAYS" />
               </children>
               <padding>
                  <Insets right="5.0" />
               </padding>
            </HBox>
            <HBox layoutX="10.0" layoutY="90.0" spacing="10.0">
               <children>
                  <Label fx:id="occupiedAreaLabel" prefHeight="17.0" prefWidth="174.0" text="Area occupied">
                     <HBox.margin>
                        <Insets left="10.0" top="3.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="occupiedAreaTextField" HBox.hgrow="ALWAYS" />
               </children>
               <padding>
                  <Insets right="5.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
